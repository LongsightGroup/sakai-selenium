<elements>
	<anchors>
		<anchor>		
			<generic>//a[@id=element]</generic>
		</anchor>
		<anchor>		
			<generic>//a[@title=element]</generic>
		</anchor>
		<anchor>		
			<generic>//a/span[normalize-space()=element and @class!='skip']</generic>
		</anchor>
		<anchor>		
			<generic>//a[normalize-space(text())=element]</generic>
		</anchor>
		<anchor>		
			<generic>//a[normalize-space()=element]</generic>
		</anchor>
		<anchor>		
			<generic>//img[@alt=element]</generic>
		</anchor>
		<anchor>		
			<assignments>//h4[normalize-space()=element]</assignments>
		</anchor>
		<anchor>		
			<dropbox>//li[contains(text(), element)]</dropbox>
		</anchor>
		<anchor>		
			<preferences>//a[@class=element]</preferences>
		</anchor>				
		<anchor>		
			<resources>//li[contains(text(), element)]</resources>
		</anchor>		
		<anchor>		
			<signup>//label[@id=element]</signup>
		</anchor>				
	</anchors>

	<bodytexts>
		<bodytext>
			<generic>//body[contains(normalize-space(), element)]</generic>
		</bodytext>
	</bodytexts>

	<buttons>
